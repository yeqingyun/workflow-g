    <userTask id="mb_deptLeaderAudiTask" name="部门领导审批" default="flow5" activiti:assignee="${processHelpService.getOrgMaster(curUserId)}"></userTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="mb_deptLeaderAudiTask"></sequenceFlow>
    <userTask id="mb_projectAudiTask" name="项目负责人审批" default="flow20" activiti:assignee="${projectLeaderId}"></userTask>
    <sequenceFlow id="flow5" name="同意" sourceRef="mb_deptLeaderAudiTask" targetRef="mb_developManagerAudiTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${deptLeaderAudi == '0'}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent2" name="End"></endEvent>
    <userTask id="mb_developManagerAudiTask" name="开发部经理审批" default="flow19" activiti:assignee="${bpmConfProcessRoleService.getSpecialRoleMaster(&quot;开发部经理审批&quot;,execution)}"></userTask>
    <sequenceFlow id="flow19" name="同意" sourceRef="mb_developManagerAudiTask" targetRef="mb_projectAudiTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${developManagerAudi == '0'}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="mb_feedbackAudiTask" name="项目开发人员处理" activiti:assignee="${projectLeaderAccount}"></userTask>
    <sequenceFlow id="flow20" name="同意" sourceRef="mb_projectAudiTask" targetRef="mb_feedbackAudiTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${projectManagerAudi == '0'}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="mb_applyUserConfirmTask" name="发起人确认" default="flow22" activiti:assignee="${applyUserId}"></userTask>
    <sequenceFlow id="flow21" sourceRef="mb_feedbackAudiTask" targetRef="mb_applyUserConfirmTask"></sequenceFlow>
    <sequenceFlow id="flow22" name="验证通过" sourceRef="mb_applyUserConfirmTask" targetRef="endevent2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${endProcessAudi == '0'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow23" name="验证不通过" sourceRef="mb_applyUserConfirmTask" targetRef="mb_feedbackAudiTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${endProcessAudi == '1'}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="nmb_updateSoftwareTask" name="修改申请" default="flow28" activiti:assignee="${applyUserId}"></userTask>
    <sequenceFlow id="flow24" name="不同意" sourceRef="mb_developManagerAudiTask" targetRef="nmb_updateSoftwareTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${developManagerAudi == '1'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow26" name="不同意" sourceRef="mb_projectAudiTask" targetRef="nmb_updateSoftwareTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${projectManagerAudi == '1'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow27" name="不同意" sourceRef="mb_deptLeaderAudiTask" targetRef="nmb_updateSoftwareTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${deptLeaderAudi == '1'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow28" name="修改申请" sourceRef="nmb_updateSoftwareTask" targetRef="mb_deptLeaderAudiTask"></sequenceFlow>
