  <process id="L-Account-Handle" name="户口办理申请流程" isExecutable="true">
    <documentation>1、	申请条件：申请办理调户手续需在本司至少服务满一年，且满足绩效要求。&lt;br/&gt;
2、	调户类型：
调干：人事档案和户籍在同一个地方，档案内有报到证、转正定级等材料；户籍必须为非农业户籍； 
招工：只调户口不调档案；&lt;br/&gt;
3、	未成年子女随迁：子女必须跟申请人同一个户口本，且没有超生情况。&lt;br/&gt;
4、	员工信息资料要求：调户过程中负责提供真实合法有效的信息和资料；如伪造、变造或故意提供虚假材料造成公司出现严重后果，其责任全部由申请人本人承担。如涉嫌犯罪的，将移送司法机关依法处理。</documentation>
    <startEvent id="startevent1" name="Start" activiti:initiator="applyUserId"></startEvent>
    <userTask id="mb_achievementsManAudiTask" name="绩效主管审批" default="flow7" activiti:assignee="${bpmConfProcessRoleService.getSpecialRoleMaster(&quot;绩效主管&quot;,execution)}"></userTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="mb_achievementsManAudiTask"></sequenceFlow>
    <userTask id="mb_departmentleaderAudiTask" name="部门经理审批" default="flow3" activiti:assignee="${processHelpService.getOrgLeader(applyUserId,1)}"></userTask>
    <userTask id="mb_inspectorgeneralAudiTask" name="总监审批" default="flow20" activiti:assignee="${processHelpService.getOrgLeader(applyUserId,2)}"></userTask>
    <sequenceFlow id="flow3" name="同意" sourceRef="mb_departmentleaderAudiTask" targetRef="mb_inspectorgeneralAudiTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${departmentleaderAudi=='0'}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="mb_deputygeneralAudiTask" name="主管副总审批" default="flow5" activiti:assignee="${processHelpService.getOrgLeader(applyUserId,3)}"></userTask>
    <userTask id="mb_humanresourcesAudiTask" name="人力资源专员" activiti:assignee="${bpmConfProcessRoleService.getSpecialRoleMaster(&quot;人力资源专员&quot;,execution)}"></userTask>
    <sequenceFlow id="flow5" name="同意" sourceRef="mb_deputygeneralAudiTask" targetRef="mb_humanresourcesAudiTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${deputygeneralAudi=='0'}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow6" sourceRef="mb_humanresourcesAudiTask" targetRef="endevent1"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow7" name="同意" sourceRef="mb_achievementsManAudiTask" targetRef="exclusivegateway1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${achievementsManAudi=='0'}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="updateAHAudiTask" name="户口办理修改申请" default="flow11" activiti:assignee="${applyUserId}"></userTask>
    <sequenceFlow id="flow10" name="不同意" sourceRef="mb_achievementsManAudiTask" targetRef="updateAHAudiTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${achievementsManAudi=='1'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow11" name="不办理" sourceRef="updateAHAudiTask" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${continueToHandle == '2'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow12" name="继续办理" sourceRef="updateAHAudiTask" targetRef="mb_achievementsManAudiTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${continueToHandle == '1'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow13" name="不同意" sourceRef="mb_departmentleaderAudiTask" targetRef="updateAHAudiTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${departmentleaderAudi=='1'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow14" name="不同意" sourceRef="mb_inspectorgeneralAudiTask" targetRef="updateAHAudiTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${inspectorgeneralAudi=='1'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow15" name="不同意" sourceRef="mb_deputygeneralAudiTask" targetRef="updateAHAudiTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${deputygeneralAudi=='1'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow18" sourceRef="exclusivegateway1" targetRef="mb_departmentleaderAudiTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isManager=='0'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow19" sourceRef="exclusivegateway1" targetRef="mb_inspectorgeneralAudiTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isManager=='1'}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow20" name="同意" sourceRef="mb_inspectorgeneralAudiTask" targetRef="exclusivegateway2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${inspectorgeneralAudi=='0'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow21" sourceRef="exclusivegateway2" targetRef="mb_deputygeneralAudiTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isManager=='1'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow22" sourceRef="exclusivegateway2" targetRef="mb_humanresourcesAudiTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isManager=='0'}]]></conditionExpression>
    </sequenceFlow>
  </process>
