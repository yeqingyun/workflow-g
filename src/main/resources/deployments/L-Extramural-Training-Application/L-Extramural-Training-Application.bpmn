      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${hrViceChairmanAudi == '0'}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="mb_ifEndApplicationTask" name="是否结束流程" activiti:assignee="${applyUserId}"></userTask>
