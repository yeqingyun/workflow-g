    <sequenceFlow id="flow20" sourceRef="mb_staffConfirmedTask" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="n_flow21" sourceRef="mb_ifEndApplicationTask" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${ifEndApplication=='1'}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="nmb_updateApplicationInfoTask" name="修改申请信息" activiti:assignee="${applyUserId}"></userTask>
    <sequenceFlow id="flow22" sourceRef="mb_ifEndApplicationTask" targetRef="nmb_updateApplicationInfoTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${ifEndApplication=='0'}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow23" sourceRef="exclusivegateway1" targetRef="mb_inspectorgeneralAudiTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${managerType == '1'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow24" sourceRef="exclusivegateway1" targetRef="mb_hrTrainAttachAudiTask">
