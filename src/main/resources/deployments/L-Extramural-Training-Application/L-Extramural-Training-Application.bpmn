    </sequenceFlow>
    <sequenceFlow id="flow25" sourceRef="exclusivegateway1" targetRef="mb_departmentleaderAudiTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${managerType == '0' }]]></conditionExpression>
    </sequenceFlow>
