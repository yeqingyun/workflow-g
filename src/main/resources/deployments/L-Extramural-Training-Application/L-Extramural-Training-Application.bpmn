    <userTask id="mb_deputygeneralAudiTask" name="主管副总" default="flow6" activiti:assignee="${processHelpService.getOrgLeader(applyUserId,3)==bpmConfProcessRoleService.getSpecialRoleMaster(&quot;HR副总裁&quot;,execution)?null:processHelpService.getOrgLeader(applyUserId,3)}"></userTask>
    <userTask id="mb_hrViceChairmanAudiTask" name="HR副总裁" default="flow7" activiti:assignee="${bpmConfProcessRoleService.getSpecialRoleMaster(&quot;HR副总裁&quot;,execution)} "></userTask>
    <userTask id="mb_staffConfirmedTask" name="员工确认" activiti:assignee="${applyUserId}"></userTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow2" sourceRef="mb_departmentleaderAudiTask" targetRef="mb_inspectorgeneralAudiTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${departmentleaderAudi == '0'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow3" sourceRef="mb_inspectorgeneralAudiTask" targetRef="mb_hrTrainAttachAudiTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${inspectorgeneralAudi == '0'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow6" sourceRef="mb_deputygeneralAudiTask" targetRef="mb_hrViceChairmanAudiTask">
